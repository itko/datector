{
  "manifest_version": 2,

  "name": "Datector",
  "description": "This extension creates links out of dates listed on a website",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.js", "get_text.js"]
    }
  ],
}